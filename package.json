{
  "name": "moveplus_game_service",
  "version": "0.0.1",
  "description": "Move Plus Game Service",
  "author": "Moveplus",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node index.js",
    "build": "rm -rf .game && babel -d .game . -s --ignore node_modules --copy-files && rm -rf .game/.git && cp .env .game/",
    "start": "node index.js",
    "lint": "eslint --ext .js --ignore-path ./game --ignore-path .gitignore .",
    "test2": "NODE_ENV=test jest",
    "seed-data": "babel-node -r dotenv/config models/seeders/dbSeeder.js",
    "test": "babel-node -r dotenv/config logs/test.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "file-loader": "^6.2.0",
    "jest": "^28.1.1",
    "jest-express": "^1.10.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cls-rtracer": "^2.6.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ethers": "^5.6.9",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-rest-routerss": "^1.0.0",
    "express-winston": "^4.2.0",
    "ioredis": "^4.28.2",
    "moment": "^2.29.4",
    "mongoose": "^6.5.3",
    "rabbitmq-client": "^3.1.1",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.2",
    "ulid": "^2.3.0",
    "validatorjs": "^3.22.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  }
}
